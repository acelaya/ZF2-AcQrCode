<?php
/** @var Zend\View\Renderer\PhpRenderer $this */
$additionalAttribs = '';
foreach ($this->attribs as $key => $value) {
    $additionalAttribs .= sprintf('%s="%s" ', $key, $value);
}
$endTag = $this->doctype()->isXhtml() ? '/' : '';
?>
<img src="<?php echo $this->src ?>" <?php echo $additionalAttribs ?> <?php echo $endTag ?>>
