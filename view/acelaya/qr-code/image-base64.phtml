<?php
/** @var Zend\View\Renderer\PhpRenderer $this */
$additionalAttribs = '';
foreach ($this->attribs as $key => $value) {
    $additionalAttribs .= sprintf('%s="%s" ', $key, $value);
}
$endTag = $this->doctype()->isXhtml() ? '/' : '';
$src = sprintf('data:%s;base64,%s', $this->contentType, $this->base64);
?>
<img src="<?php echo $src ?>" <?php echo $additionalAttribs ?> <?php echo $endTag ?>>
