{
    "name": "acelaya/zf2-acqrcode",
    "description": "QR Code generation module for Zend Framework 2 based on Endroid QR code",
    "license": "MIT",
    "authors": [
        {
            "name": "Alejandro Celaya Alastru√©",
            "email": "alejandro@alejandrocelaya.com",
            "homepage": "https://www.alejandrocelaya.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "zf2",
        "module",
        "qr",
        "php",
        "endroid"
    ],
    "homepage": "https://github.com/acelaya/ZF2-AcQrCode",
    "require": {
        "php" : "^5.6 | ^7.0",
        "endroid/qrcode": "^1.6",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-router": "^3.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-view": "^2.8",
        "zendframework/zend-modulemanager": "^2.7",
        "zendframework/zend-http": "^2.6",
        "zendframework/zend-eventmanager": "^3.0",
        "zendframework/zend-stdlib": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 | ^6.0",
        "squizlabs/php_codesniffer": "^2.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Acelaya\\QrCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acelaya\\QrCode\\Test\\": "test/"
        }
    },
    "extra": {
        "zf": {
            "module": "Acelaya\\QrCode",
            "config-provider": "Acelaya\\QrCode\\ConfigProvider"
        }
    }
}
